{
  "name": "ptz-camera-facetracker",
  "version": "0.1.0",
  "description": "A electron app, powered by Vue and Typescript that enables any ONVIF PTZ Camera to track a subject using their face.",
  "author": "Michael Mesquita <mannolaur@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/Midlight25/ptz-camera-facetracker"
  },
  "keywords": [
    "vue",
    "vue 3",
    "face tracking",
    "ptz camera",
    "nodejs",
    "electron",
    "electron js",
    "onvif"
  ],
  "bugs": {
    "url": "https://github.com/Midlight25/ptz-camera-facetracker/issues"
  },
  "homepage": "https://github.com/Midlight25/ptz-camera-facetracker/README.md",
  "private": true,
  "main": "electron/main.js",
  "scripts": {
    "start": "electron ./electron/main.js",
    "build:dev": "vue-cli-service build",
    "build:dev:watch": "vue-cli-service build --watch",
    "build:prod": "vue-cli-service build --modern --production",
    "test:unit": "vue-cli-service test:unit",
    "lint": "vue-cli-service lint",
    "dist": "npm run build:prod && electron-builder",
    "dist:mac": "npm run build:prod && electron-builder --mac",
    "dist:linux": "npm run build:prod && electron-builder --linux",
    "dist:windows": "npm run build:prod && electron-builder --windows",
    "dist:all": "npm run build:prod && electron-builder --mac --linux --windows",
    "serve": "concurrently --kill-others \"npm run build:dev:watch\" \"npm run start\""
  },
  "dependencies": {
    "core-js": "^3.6.5",
    "dotenv": "^8.2.0",
    "onvif": "github:agsh/onvif",
    "opencv-wasm": "^4.3.0-10",
    "pug": "^3.0.2",
    "pug-plain-loader": "^1.1.0",
    "vue": "^3.2.4"
  },
  "devDependencies": {
    "@types/chai": "^4.2.11",
    "@types/mocha": "^5.2.4",
    "@typescript-eslint/eslint-plugin": "^4.29.1",
    "@typescript-eslint/parser": "^4.29.1",
    "@vue/cli-plugin-babel": "~4.5.0",
    "@vue/cli-plugin-eslint": "^4.5.0",
    "@vue/cli-plugin-typescript": "~4.5.0",
    "@vue/cli-plugin-unit-mocha": "^4.5.13",
    "@vue/cli-service": "^4.5.13",
    "@vue/compiler-sfc": "^3.2.2",
    "@vue/eslint-config-typescript": "^7.0.0",
    "@vue/test-utils": "^2.0.0-0",
    "chai": "^4.1.2",
    "concurrently": "^6.2.1",
    "electron": "^11.0.3",
    "electron-builder": "^22.9.1",
    "electron-reloader": "^1.1.0",
    "eslint": "^6.7.2",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-vue": "^7.16.0",
    "prettier": "^2.3.2",
    "sass": "^1.26.5",
    "sass-loader": "^8.0.2",
    "typescript": "~4.1.5",
    "worker-plugin": "^5.0.1"
  }
}
